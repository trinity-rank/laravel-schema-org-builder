{
    "name": "trinityrank/laravel-schema-org-builder",
    "description": "Schema.org builder. Creating multiple nodes and makes relations between them.",
    "keywords": [
        "php",
        "laravel",
        "schema",
        "json-ld"
    ],
    "homepage": "https://github.com/trinity-rank/laravel-schema-org-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "TrinityRank DevTeam",
            "email": "trinitydevbg@gmail.com",
            "role": "Team"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "spatie/schema-org": "^3.8"
    },
    "require-dev": {
        "spatie/laravel-ray": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\LaravelSchemaOrgBuilder\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\LaravelSchemaOrgBuilder\\LaravelSchemaOrgBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
